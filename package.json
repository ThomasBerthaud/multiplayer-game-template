{
	"name": "multiplayer-game-template",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest",
		"format": "prettier --write --plugin prettier-plugin-svelte .",
		"db-types": "supabase gen types typescript --linked > src/lib/application/database.types.ts",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint --cache --fix",
		"*.{js,ts,svelte,json,css,md}": "npm run format"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.4.2",
		"@supabase/auth-helpers-sveltekit": "^0.12.0",
		"@supabase/supabase-js": "^2.39.3",
		"hashids": "^2.2.11",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.2",
		"@skeletonlabs/skeleton": "^2.8.0",
		"@skeletonlabs/tw-plugin": "^0.3.1",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/node": "^20.5.6",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.31",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.1.2",
		"supabase": "^1.136.3",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-plugin-tailwind-purgecss": "^0.2.0",
		"vitest": "^1.2.2"
	}
}
